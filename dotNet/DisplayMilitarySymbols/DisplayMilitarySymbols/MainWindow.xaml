<Window x:Class="DisplayMilitarySymbols.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DisplayMilitarySymbols"
        xmlns:esri="http://schemas.esri.com/arcgis/runtime/2013"
        mc:Ignorable="d" WindowState="Maximized"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition Height="40" />
        </Grid.RowDefinitions>
        <esri:MapView x:Name="MyMapView" />

        <UniformGrid Grid.Row="1" Columns="2">
            <Button x:Name="btnPrevious" Content="Previous" Click="Button_Click" />
            <Button x:Name="btnNext" Content="Next" Click="Button_Click" />
        </UniformGrid>
        
    </Grid>
</Window>
